{
  "name": "designer-planta-baixa-para-cftv",
  "private": true,
  "version": "0.0.0",
  "homepage": "./",
  "main": "electron.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "electron:start": "electron .",
    "electron:dev": "npm-run-all -p dev electron:start",
    "electron:build": "npm run build && electron-builder --never"
  },
  "dependencies": {
    "electron-is-dev": "^3.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "concurrently": "^9.2.0",
    "electron": "^37.2.4",
    "electron-builder": "^26.0.12",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "wait-on": "^8.0.4"
  },
  "build": {
    "appId": "com.example.designer-planta-baixa-para-cftv",
    "productName": "Designer de Planta Baixa para CFTV",
    "files": [
      "dist/**/*",
      "electron.cjs"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "release"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
